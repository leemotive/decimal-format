{
  "name": "decimal-format",
  "version": "4.0.0",
  "description": "format decimal",
  "jsnext:main": "es/index.mjs",
  "browser": "dist/decimalFormat.min.js",
  "main": "./lib/index.js",
  "module": "./es/index.mjs",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "umd": "./dist/decimalFormat.min.js",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib",
    "es",
    "dist",
    "typings"
  ],
  "scripts": {
    "build": "rm -rf dist/ es/ lib/ coverage/ typings/ && rollup -c && uglifyjs dist/decimalFormat.js -o dist/decimalFormat.min.js",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@vitest/coverage-v8": "3.0.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "rollup": "^4.38.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.2",
    "uglify-es": "^3.3.9",
    "vitest": "^3.0.9"
  },
  "keywords": [
    "decimal",
    "format"
  ]
}
